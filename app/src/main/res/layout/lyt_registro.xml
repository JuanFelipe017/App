<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/large_triangles"
    tools:context=".Registro">


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="49dp"
        android:layout_height="48dp"
        android:layout_marginEnd="4dp"
        android:background="@color/white"
        android:backgroundTint="@color/white"
        android:outlineAmbientShadowColor="#EAE1E1"
        android:outlineSpotShadowColor="@color/white"
        android:src="@drawable/user_alt"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="2dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="307dp"
        android:layout_height="110dp"
        android:fontFamily="@font/alfa_slab_one_1"
        android:text="@string/textView10"
        android:textAlignment="center"
        android:textColor="@color/material_dynamic_neutral_variant80"
        android:textSize="40sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.003" />

    <EditText
        android:id="@+id/editTextText"
        android:layout_width="238dp"
        android:layout_height="54dp"
        android:ems="10"
        android:hint="@string/editTextText"
        android:inputType="text"
        android:textColor="@color/material_dynamic_neutral_variant80"
        android:textColorHint="@color/material_dynamic_neutral_variant80"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        app:layout_constraintVertical_bias="0.062" />

    <EditText
        android:id="@+id/editTextText2"
        android:layout_width="238dp"
        android:layout_height="61dp"
        android:ems="10"
        android:hint="@string/editTextText2"
        android:inputType="text"
        android:textColor="@color/material_dynamic_neutral_variant80"
        android:textColorHint="@color/material_dynamic_neutral_variant80"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextText"
        app:layout_constraintVertical_bias="0.018" />

    <EditText
        android:id="@+id/editTextTextEmailAddress2"
        android:layout_width="238dp"
        android:layout_height="69dp"
        android:ems="10"
        android:hint="@string/editTextTextEmailAddress"
        android:inputType="textEmailAddress"
        android:textColor="@color/material_dynamic_neutral_variant80"
        android:textColorHint="@color/material_dynamic_neutral_variant80"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/opciones_sexo"
        app:layout_constraintVertical_bias="0.046" />

    <RadioGroup
        android:id="@+id/opciones_sexo"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextText2"
        app:layout_constraintVertical_bias="0.035">

        <RadioButton
            android:id="@+id/radio_femenino"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:checked="false"
            android:text="@string/radio_femenino"
            android:textColor="@color/material_dynamic_neutral_variant80"
            android:textColorHighlight="#F44336"
            android:textColorLink="#F44336"
            tools:layout_editor_absoluteX="118dp"
            tools:layout_editor_absoluteY="0dp" />

        <RadioButton
            android:id="@+id/radio_masculino"
            android:layout_width="109dp"
            android:layout_height="match_parent"
            android:checked="true"
            android:text="@string/radio_masculino"
            android:textColor="@color/material_dynamic_neutral_variant80"
            android:textColorHighlight="#F44336"
            android:textColorLink="#F44336"
            tools:layout_editor_absoluteX="202dp"
            tools:layout_editor_absoluteY="276dp" />
    </RadioGroup>

    <Spinner
        android:id="@+id/spinnerNacionalidad"
        android:layout_width="261dp"
        android:layout_height="57dp"
        android:entries="@array/nacionalidades"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPassword2"
        app:layout_constraintVertical_bias="0.158" />

    <EditText
        android:id="@+id/editTextTextPassword2"
        android:layout_width="241dp"
        android:layout_height="68dp"
        android:layout_marginTop="20dp"
        android:ems="10"
        android:hint="@string/editTextTextPassword"
        android:inputType="textPassword"
        android:textColorHint="@color/material_dynamic_neutral_variant80"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.488"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextEmailAddress2"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/Confirmacion_btn"
        android:layout_width="198dp"
        android:layout_height="87dp"
        android:backgroundTint="@color/material_dynamic_tertiary100"
        android:text="@string/Confirmacion_btn"
        android:textAlignment="center"
        android:textColor="@color/material_dynamic_neutral10"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.948" />
</androidx.constraintlayout.widget.ConstraintLayout>